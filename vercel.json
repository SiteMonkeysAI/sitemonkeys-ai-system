{
  "functions": {
    "api/load-vault.py": {
      "runtime": "@vercel/python@3.9.13"
    },
    "api/chat.py": {
      "runtime": "@vercel/python@3.9.13"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
